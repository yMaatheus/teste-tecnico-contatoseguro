{
  "name": "vite-react-testing",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "start": "yarn dev",
    "build": "tsc && vite build",
    "preview": "vite preview --host 0.0.0.0 --port 3000",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint:styles": "npx stylelint '**/*.css'",
    "lint": "npx eslint --ext .js,.jsx,.ts,.tsx, src",
    "compose": "docker-compose up -d",
    "compose:down": "docker-compose down",
    "compose:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "compose:dev:down": "docker-compose -f docker-compose.dev.yml down"
  },
  "dependencies": {
    "axios": "^1.3.5",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-router-dom": "^6.10.0",
    "sort-by": "^1.2.0",
    "yup-locale-pt": "^0.0.9"
  },
  "devDependencies": {
    "@hookform/resolvers": "^3.0.1",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/js-cookie": "^3.0.3",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-istanbul": "^0.30.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-no-mixed-operators": "^1.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "jsdom": "^21.1.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "react-icons": "^4.8.0",
    "stylelint": "^15.4.0",
    "stylelint-config-standard": "32.0.0",
    "stylelint-order": "^6.0.3",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vitest": "^0.30.1",
    "yup": "^1.1.1"
  }
}
